{
    "name": "knplabs/doctrine-behaviors",
    "description": "Doctrine2 behavior traits",
    "keywords": [
        "behaviors", "doctrine2", "timestampable", "translatable", "filterable", "blameable", "softdeletable", "tree"
    ],
    "homepage": "http://knplabs.com",
    "license": "MIT",
    "authors": [
        { "name": "Knplabs", "homepage": "http://knplabs.com" }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/common": "^2.7",
        "doctrine/dbal": "^2.9",
        "doctrine/orm": "^2.6",
        "behat/transliterator": "^1.2",
        "symfony/dependency-injection": "^4.4|^5.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-pdo_mysql": "*",
        "ext-pdo_pgsql": "*",
        "phpunit/phpunit": "^8.5",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "symplify/easy-coding-standard": "^7.1",
        "phpstan/phpstan": "^0.12.2",
        "doctrine/annotations": "^1.8",
        "symfony/http-kernel": "^4.4|^5.0",
        "symplify/package-builder": "^7.1",
        "rector/rector": "^0.6.1",
        "doctrine/doctrine-bundle": "^2.0",
        "symplify/phpstan-extensions": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Knp\\DoctrineBehaviors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Knp\\DoctrineBehaviors\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "ecs check src tests --ansi",
        "fix-cs": "ecs check src tests --fix --ansi",
        "phpstan": "phpstan analyse src tests --ansi --error-format symplify"
    }
}
